  <% end %>
  <div class="absolute top-40">
    <div class="flex flex-col">
      <div class="h-40">
        <%= image_tag 'woodframe03.jpg' %>
      </div>
      <div class="h-40">
        <%= image_tag 'woodframe03.jpg' %>
      </div>
      <div class="h-40">
        <%= image_tag 'woodframe03.jpg' %>
      </div>
    </div>
  </div>
</div>
